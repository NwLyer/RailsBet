    <% x = 0 %>
    <% values = @items.values %>
    <% loop do %>
    <%  @foto = values[rand(values.size)] %>
    <%  @name = @items.key(@foto) %>
    <%  break if x == 93 %>
    <div id="box" class="relative">
      <%  if x == 83 %>
      <%  @winnername = @name %>
      <%  @winnerfoto = @foto %>
      <%  end %>
        <div id="image">
          <img src= "/assets/<%= @foto %>" alt="<%= @name %>" width="90" height="90" >
        </div>
        <p><%= @name%></p>
</div>
    <% x+= 1 %>
    <% end %>
    </div>
<div id="showwinner">
